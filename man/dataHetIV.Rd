\name{dataHetIV}
\alias{dataHetIV}
\docType{data}
\title{
Simmulated Dataset
}
\description{
A dataset simmulated to exemplify the use of the \code{hetErrorsIV()} function.
}
\usage{data("dataHetIV")}
\format{
  A data frame with 2500 observations on the following 4 variables.
  \describe{
    \item{\code{y}}{a numeric vector representing the dependent variable.}
    \item{\code{X1}}{a numeric vector representing a normally distributed exogenous variable.}
    \item{\code{X2}}{a numeric vector representing a normally distributed exogenous variable.}
    \item{\code{P}}{a numeric vector representing a normally distributed endogenous variable.}
  }
}
\keyword{datasets}
